<?xml version="1.0" encoding="utf-8"?>
<resources> 
    
    <declare-styleable name="com.baidu.mobads.AdView">
		<attr name="adSize" format="integer" />
	</declare-styleable> 
	<!-- net.wutnews.app.framge.widget.CircleImageView圆形ImageView控件的属性 -->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <!-- net.wutnews.app.framge.widget.TitleBarView自定义标题栏的属性 -->
    <declare-styleable name="TitleBarView">
        <attr name="center_text" format="string"/>
        <attr name="left_text" format="string"/>
        <attr name="right_text" format="string"/>
        <attr name="left_image" format="reference"/>
        <attr name="right_image" format="reference"/>
    </declare-styleable>
    <!-- 自定义夜间模式 -->
    <declare-styleable name="NightStyle">
        <attr name="bookimage" format="reference|color" />
        <attr name="tvcolor" format="reference|color" />
    </declare-styleable>

</resources>
